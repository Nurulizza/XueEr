<?xml version='1.0' encoding='UTF-8'?>
<model name="KIR2DL4_to_Cytokines" xmlns="http://www.cellml.org/cellml/1.1#" xmlns:cellml="http://www.cellml.org/cellml/1.1#" xmlns:xlink="http://www.w3.org/1999/xlink">
    <import xlink:href="Receptor_activation.cellml">
        <component component_ref="receptor_activation" name="receptor_activation"/>
    </import>
    <import xlink:href="FC_epsilon.cellml">
        <component component_ref="FCepsilonRI" name="FCepsilonRI"/>
    </import>
    <import xlink:href="PI3K.cellml">
        <component component_ref="PI3K" name="PI3K"/>
    </import>
    <import xlink:href="NFATMyocyte_Tomida_Protocol_Submodel.cellml">
        <component component_ref="geometry" name="geometry"/>
        <component component_ref="calcium" name="calcium"/>
        <component component_ref="calcineurin" name="calcineurin"/>
        <component component_ref="NFAT_Cycling" name="NFAT_Cycling"/>
        <component component_ref="NFAT_Cycling_equations" name="NFAT_Cycling_equations"/>
    </import>
    <import xlink:href="NFAT_Cytokines.cellml">
        <component component_ref="NFAT_Cytokines" name="NFAT_Cytokines"/>
    </import>
    <!--
*******************************************************
***         U   N   I   T   S                       ***
*******************************************************
-->
    <units name="s">
        <unit units="second"/>
    </units>
    <units name="uM">
        <unit prefix="micro" units="mole"/>
        <unit exponent="-1" units="liter"/>
    </units>
    <units name="per_s">
        <unit exponent="-1" units="s"/>
    </units>
    <units name="uM_per_s">
        <unit units="uM"/>
        <unit exponent="-1" units="s"/>
    </units>
    <units name="per_uM_per_s">
        <unit exponent="-1" units="uM"/>
        <unit exponent="-1" units="s"/>
    </units>
    <units name="per_uM_s">
        <unit exponent="-1" units="uM"/>
        <unit exponent="-1" units="s"/>
    </units>
    <!--
*********************************************************
***       C   O   M   P   O   N   E   N   T   S       ***
*********************************************************
-->
    <component name="environment">
        <variable name="t" public_interface="out" units="s"/>
    </component>
    <component name="HGDL4_FC">
        <variable name="t" public_interface="in" units="s"/>
        <variable name="J21" units="uM_per_s"/>
        <variable name="J1" public_interface="in" units="uM_per_s"/>
        <variable name="J5" public_interface="in" units="uM_per_s"/>
        <variable name="J_HGDL4" public_interface="in" units="uM_per_s"/>
        <variable initial_value="1" name="FC" public_interface="out" units="uM"/>
        <variable initial_value="0" name="HGDL4_FC" public_interface="out" units="uM"/>
        <variable initial_value="0" name="HGDL4" units="uM"/>
        <variable initial_value="0.016521" name="k_f21" units="per_uM_per_s"/>
        <variable initial_value="0.0517365" name="k_r21" units="per_s"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>J21</ci>
                <apply>
                    <minus/>
                    <apply>
                        <times/>
                        <ci>k_f21</ci>
                        <ci>J_HGDL4</ci>
                        <ci>FC</ci>
                    </apply>
                    <apply>
                        <times/>
                        <ci>k_r21</ci>
                        <ci>HGDL4_FC</ci>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>HGDL4</ci>
                </apply>
                <apply>
                    <minus/>
                    <ci>J_HGDL4</ci>
                    <ci>J21</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>FC</ci>
                </apply>
                <apply>
                    <plus/>
                    <apply>
                        <minus/>
                        <ci>J21</ci>
                    </apply>
                    <ci>J5</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>HGDL4_FC</ci>
                </apply>
                <apply>
                    <minus/>
                    <ci>J21</ci>
                    <ci>J1</ci>
                </apply>
            </apply>
        </math>
    </component>
    <component name="Cytokines">
        <variable name="t" public_interface="in" units="s"/>
        <variable name="NFATN_n" public_interface="in" units="uM"/>
        <variable name="NFATN_c" public_interface="in" units="uM"/>
        <variable name="NFATp_n" public_interface="in" units="uM"/>
        <variable name="NFATp_c" public_interface="in" units="uM"/>
        <variable name="k_f5" public_interface="in" units="per_s"/>
        <variable name="k_f6" public_interface="in" units="per_s"/>
        <variable name="IFN" public_interface="in" units="uM"/>
        <variable name="TNF" public_interface="in" units="uM"/>
        <variable name="k_f19" public_interface="in" units="per_s"/>
        <variable name="k_f20" public_interface="in" units="per_s"/>
    </component>
    <connection>
        <map_components component_1="receptor_activation" component_2="environment"/>
        <map_variables variable_1="t" variable_2="t"/>
    </connection>
    <connection>
        <map_components component_1="HGDL4_FC" component_2="environment"/>
        <map_variables variable_1="t" variable_2="t"/>
    </connection>
    <connection>
        <map_components component_1="FCepsilonRI" component_2="environment"/>
        <map_variables variable_1="t" variable_2="t"/>
    </connection>
    <connection>
        <map_components component_1="Cytokines" component_2="environment"/>
        <map_variables variable_1="t" variable_2="t"/>
    </connection>
    <connection>
        <map_components component_1="environment" component_2="PI3K"/>
        <map_variables variable_1="t" variable_2="t"/>
    </connection>
    <connection>
        <map_components component_1="environment" component_2="geometry"/>
        <map_variables variable_1="t" variable_2="t"/>
    </connection>
    <connection>
        <map_components component_1="environment" component_2="calcium"/>
        <map_variables variable_1="t" variable_2="t"/>
    </connection>
    <connection>
        <map_components component_1="environment" component_2="calcineurin"/>
        <map_variables variable_1="t" variable_2="t"/>
    </connection>
    <connection>
        <map_components component_1="environment" component_2="NFAT_Cycling"/>
        <map_variables variable_1="t" variable_2="t"/>
    </connection>
    <connection>
        <map_components component_1="environment" component_2="NFAT_Cycling_equations"/>
        <map_variables variable_1="t" variable_2="t"/>
    </connection>
    <connection>
        <map_components component_1="environment" component_2="NFAT_Cytokines"/>
        <map_variables variable_1="t" variable_2="t"/>
    </connection>
    <connection>
        <map_components component_1="PI3K" component_2="FCepsilonRI"/>
        <map_variables variable_1="J7" variable_2="J7"/>
        <map_variables variable_1="pGrb2" variable_2="pGrb2"/>
    </connection>
    <connection>
        <map_components component_1="receptor_activation" component_2="HGDL4_FC"/>
        <map_variables variable_1="J_HGDL4" variable_2="J_HGDL4"/>
    </connection>
    <connection>
        <map_components component_1="HGDL4_FC" component_2="FCepsilonRI"/>
        <map_variables variable_1="FC" variable_2="FC"/>
        <map_variables variable_1="J1" variable_2="J1"/>
        <map_variables variable_1="J5" variable_2="J5"/>
        <map_variables variable_1="HGDL4_FC" variable_2="HGDL4_FC"/>
    </connection>
    <connection>
        <map_components component_1="Cytokines" component_2="geometry"/>
        <map_variables variable_1="NFATN_n" variable_2="NFATN_n"/>
    </connection>
    <connection>
        <map_components component_1="calcineurin" component_2="geometry"/>
        <map_variables variable_1="t" variable_2="t"/>
        <map_variables variable_1="Ca" variable_2="Ca"/>
    </connection>
    <connection>
        <map_components component_1="calcium" component_2="geometry"/>
        <map_variables variable_1="t" variable_2="t"/>
        <map_variables variable_1="Ca" variable_2="Ca"/>
    </connection>
    <connection>
        <map_components component_1="PI3K" component_2="calcium"/>
        <map_variables variable_1="t" variable_2="t"/>
    </connection>
    <connection>
        <map_components component_1="PI3K" component_2="NFAT_Cycling_equations"/>
        <map_variables variable_1="t" variable_2="t"/>
    </connection>
    <connection>
        <map_components component_1="Cytokines" component_2="NFAT_Cycling"/>
        <map_variables variable_1="NFATN_c" variable_2="NFATN_c"/>
        <map_variables variable_1="NFATp_n" variable_2="NFATp_n"/>
        <map_variables variable_1="NFATp_c" variable_2="NFATp_c"/>
    </connection>
    <connection>
        <map_components component_1="Cytokines" component_2="NFAT_Cytokines"/>
        <map_variables variable_1="k_f5" variable_2="k_f5"/>
        <map_variables variable_1="k_f6" variable_2="k_f6"/>
        <map_variables variable_1="IFN" variable_2="IFN"/>
        <map_variables variable_1="TNF" variable_2="TNF"/>
        <map_variables variable_1="k_f19" variable_2="k_f19"/>
        <map_variables variable_1="k_f20" variable_2="k_f20"/>
    </connection>
    <connection>
        <map_components component_1="geometry" component_2="NFAT_Cytokines"/>
        <map_variables variable_1="NFATN_n" variable_2="NFATN_n"/>
        <map_variables variable_1="J_NFATNn" variable_2="J_NFATNn"/>
    </connection>
</model>
