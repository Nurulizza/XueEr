<?xml version='1.0' encoding='UTF-8'?>
<model name="KIR2DL4_singnaling" xmlns="http://www.cellml.org/cellml/1.1#" xmlns:cellml="http://www.cellml.org/cellml/1.1#" xmlns:xlink="http://www.w3.org/1999/xlink">
    <import xlink:href="FC_epsilon_F.cellml">
        <component component_ref="FCepsilonRI" name="FCepsilonRI"/>
    </import>
    <import xlink:href="Cooling_model/NFATMyocyte_TomidaProtocol_Submodel.cellml">
        <component component_ref="geometry" name="geometry"/>
